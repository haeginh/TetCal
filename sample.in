# Set the verbose
/run/verbose 2

# Set the number of threads for multi-threading mode
/run/numberOfThreads  1

# Initialize
/run/initialize

# external irradiation
/gun/particle e-
/external/dir AP
/gun/energy 0.001 MeV
/run/beamOn 100000

# internal source
/gun/particle e-
/internal/source 95
/gun/energy 0.001 MeV
/run/beamOn 100000


